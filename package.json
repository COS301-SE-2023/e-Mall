{
  "name": "e-Mall",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "docker-compose up",
    "docker:exec": "docker-compose exec",
    "app:uninstall": "yarn docker:exec app npm uninstall",
    "api:uninstall": "yarn docker:exec api pip uninstall",
    "app:install": "yarn docker:exec app npm install",
    "api:install": "yarn docker:exec api pip install",
    "api:freeze": "yarn docker:exec api pip freeze> api/requirements.txt",
    "api:superuser": "yarn docker:exec api py api/manage.py createsuperuser"
  }
}
