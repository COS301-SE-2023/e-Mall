{
  "name": "e-Mall",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:other": "yarn install && $TERMINAL ng serve --port 4200 --configuration=development & $TERMINAL docker-compose up",
    "start": "yarn install && start cmd /k \"ng serve --port 4200 --configuration=development\" & start cmd /k \"docker-compose up\"",
    "docker:exec": "docker-compose exec",
    "api:uninstall": "yarn docker:exec api pip uninstall",
    "api:install": "yarn docker:exec api pip install",
    "api:freeze": "docker-compose exec api pip freeze>../api/requirements.txt",
    "api:superuser": "yarn docker:exec api python manage.py createsuperuser",
    "api:startapp": "yarn docker:exec api python manage.py startapp",
    "api:makemigrations": "yarn docker:exec api python manage.py makemigrations",
    "api:migrate": "yarn docker:exec api python manage.py migrate",
    "api:db": "yarn api:makemigrations && yarn api:migrate",
    "api:test": "yarn docker:exec api pytest",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "add": "^2.0.6",
    "aws-amplify": "^5.2.2",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.5",
    "lint": "^1.1.2",
    "prettier": "^2.8.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "^16.0.2",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier-eslint": "^15.0.1",
    "typescript": "~4.9.4"
  },
  "license": "MIT"
}
